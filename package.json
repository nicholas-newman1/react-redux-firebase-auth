{
  "name": "react-redux-firebase-auth",
  "author": "Nicholas Newman",
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicholas-newman1/react-redux-firebase-auth"
  },
  "keywords": [
    "react",
    "redux",
    "firebase",
    "authentication"
  ],
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/react-redux-firebase-auth.esm.js",
  "size-limit": [
    {
      "path": "dist/react-redux-firebase-auth.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-redux-firebase-auth.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.0",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "husky": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^6.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/react-router-dom": "^5.3.1",
    "firebase": "^8.10.0",
    "react-hook-form": "^6.15.4",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0"
  }
}
